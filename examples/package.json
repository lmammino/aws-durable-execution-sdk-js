{
  "name": "@amzn/durable-executions-type-script-examples",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "repository": "ssh://git.amazon.com/pkg/DurableExecutionsTypeScriptExamples",
  "homepage": "https://code.amazon.com/packages/DurableExecutionsTypeScriptExamples",
  "engines": {
    "node": ">=18"
  },
  "files": [
    "dist",
    "build/source"
  ],
  "exports": {
    "./*": {
      "require": "./dist/examples/*",
      "types": "./dist/examples/*"
    }
  },
  "scripts": {
    "clean": "rm -rf build/ build coverage node_modules/ dist/",
    "release": "npm run build && mkdir -p build/durable-executions-typescript-sdk-examples && npm run copy-files",
    "prebuild": "rm -rf build/durable-executions-typescript-sdk-examples",
    "build": "tsc",
    "prerelease": "npm run generate-template",
    "copy-files": "npm run copy-lambda && npm run copy-examples && npm run copy-source && npm run copy-catalog",
    "copy-lambda": "cp -r configuration/aws_lambda build",
    "copy-examples": "cp -r dist/examples/*.js dist/examples/*.js.map build/durable-executions-typescript-sdk-examples",
    "copy-source": "mkdir -p build/source && cp -r src/examples/*.ts build/source/",
    "copy-catalog": "cp examples-catalog.json build/durable-executions-typescript-sdk-examples/",
    "generate-template": "node scripts/generate-template.js",
    "test": "npm run unit-test",
    "unit-test": "jest --collectCoverage",
    "unit-test-watch": "jest --watch",
    "prettier": "prettier --write './src/**/*.*' && prettier --write './test/**/*.*'"
  },
  "dependencies": {
    "@amzn/durable-executions-language-sdk": "file:../lambda-durable-functions-sdk-js",
    "@amzn/durable-executions-type-script-testing-library": "file:../lambda-durable-functions-testing-sdk-js",
    "@aws-sdk/client-dynamodb": "^3.777.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.23.0",
    "@tsconfig/node22": "^22.0.1",
    "@types/aws-lambda": "^8.10.145",
    "@types/jest": "^29.5.14",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.13.5",
    "eslint": "^9.23.0",
    "jest": "^29.7.0",
    "js-yaml": "^4.1.0",
    "prettier": "^3.5.2",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.29.0"
  },
  "private": true
}